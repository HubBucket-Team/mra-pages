{  
  "id": "/redis",
  "cpus": 0.5,
  "mem": 512,
  "instances": 1,
  "container": {
    "type": "DOCKER",
    "docker": {
      "image": "redis",
      "forcePullImage": true,
      "network": "BRIDGE",
      "portMappings": [
        {
            "containerPort": 6379,
            "hostPort": 6379,
            "protocol": "tcp"
        }
      ]
	}
  }
}
